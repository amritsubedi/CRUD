@model IEnumerable<WebApplication6.Models.User>
@{
    ViewData["Title"] = "Index";
}
<h2>Index</h2>
<hr />
<div>
    @foreach (var user in Model)
    {
        <dl class="dl-horizontal">
            <dt>
                Name
            </dt>
            <dd>
                @user.Name
            </dd>

            <dt>
                PhoneNumber
            </dt>
            <dd>
                @user.PhoneNumber
            </dd>
        </dl>
    }
</div>
